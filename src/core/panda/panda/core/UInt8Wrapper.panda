    The minimum value representable by a `UInt8`.
    ============================================================================
    constant MIN:UInt8 := 0
    
    ============================================================================
    The maximum value representable by a `UInt8`.
    ============================================================================
    constant MAX:UInt8 := 255

    ============================================================================
    The number of bits in a `UInt8`.
    ============================================================================
    constant BITS := 8

