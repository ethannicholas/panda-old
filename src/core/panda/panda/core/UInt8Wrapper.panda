    ============================================================================
    Creates a slice of the integers: an array ranging from `left` (inclusive) to
    `right` (exclusive).
    ============================================================================
    @class
    function [..](left:UInt8, right:UInt8):Array<UInt8> {
        def result := new Array<UInt8>()
        for v:UInt8 in left .. right
            result.append(v)
        return result
    }

    ============================================================================
    Creates a slice of the integers: an array ranging from `left` (inclusive) to
    `right` (exclusive), counting by `step`.
    ============================================================================
    @class
    function [..](left:UInt8, right:UInt8, step:Int8):Array<UInt8> {
        def result := new Array<UInt8>()
        for v in left .. right by step
            result.append(v->(UInt8))
        return result
    }

    @class 
    function [...](left:UInt8):Array<UInt8> {
        throw new UnimplementedException()
    }
    
    ============================================================================
    Creates a slice of the integers: an array ranging from `left` to `right` 
    (inclusive).
    ============================================================================
    @class
    function [...](left:UInt8, right:UInt8):Array<UInt8> {
        var result := new Array<UInt8>()
        for v:UInt8 in left ... right
            result.append(v)
        return result
    }

    ============================================================================
    Creates a slice of the integers: an array ranging from `left` to `right` 
    (inclusive), counting by `step`.
    ============================================================================
    @class
    function [...](left:UInt8, right:UInt8, step:Int8):Array<UInt8> {
        def result := new Array<UInt8>()
        for v in left ... right by step
            result.append(v->(UInt8))
        return result
    }

