package panda.core

================================================================================
Thrown when a program encounters an unrecoverable internal error.
================================================================================
class InternalError : AssertionError {
    ============================================================================
    Creates a new `InternalError`.
    ============================================================================
    init() {
    }

    ============================================================================
    Creates a new `InternalError` with a message.

    @param message the error message
    ============================================================================
    init(message:String) {
        super.init(message)
    }
}