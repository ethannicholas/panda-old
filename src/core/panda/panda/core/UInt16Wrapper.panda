    def value:UInt16
    
    ============================================================================
    Creates a slice of the integers: an array ranging from `left` (inclusive) to
    `right` (exclusive).
    ============================================================================
    @class
    function [..](left:UInt16, right:UInt16):Array<UInt16> {
        def result := new Array<UInt16>()
        for v:UInt16 in left .. right
            result.append(v)
        return result
    }

    ============================================================================
    Creates a slice of the integers: an array ranging from `left` (inclusive) to
    `right` (exclusive), counting by `step`.
    ============================================================================
    @class
    function [..](left:UInt16, right:UInt16, step:Int16):Array<UInt16> {
        def result := new Array<UInt16>()
        for v in left .. right by step
            result.append(v->(UInt16))
        return result
    }

    @class 
    function [...](left:UInt16):Array<UInt16> {
        throw new UnimplementedException()
    }
    
    ============================================================================
    Creates a slice of the integers: an array ranging from `left` to `right` 
    (inclusive).
    ============================================================================
    @class
    function [...](left:UInt16, right:UInt16):Array<UInt16> {
        var result := new Array<UInt16>()
        for v:UInt16 in left ... right
            result.append(v)
        return result
    }

    ============================================================================
    Creates a slice of the integers: an array ranging from `left` to `right` 
    (inclusive), counting by `step`.
    ============================================================================
    @class
    function [...](left:UInt16, right:UInt16, step:Int16):Array<UInt16> {
        def result := new Array<UInt16>()
        for v in left ... right by step
            result.append(v->(UInt16))
        return result
    }

