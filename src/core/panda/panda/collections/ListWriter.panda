package panda.collections

interface ListWriter<T> : CollectionWriter<T> {
    method []:=(i:Int, object:T)

    @self
    method [..]:=(start:Int, var end:Int?, step:Int, list:ListView<T>) {
        if end = null {
            if step > 0
                end := length
            else
                end := -1
        }
        self[start .. end->(Int) by step] := list
    }

    @self
    method [..]:=(start:Int, end:Int, step:Int, list:ListView<T>) {
        for i, j in start .. end by step
            self[j] := list[i]
    }

    @self
    method [...]:=(start:Int, var end:Int?, step:Int, list:ListView<T>) {
        if end = null {
            if step > 0
                end := length - 1
            else
                end := 0
        }
        self[start .. end->(Int) by step] := list
    }

    @self
    method [...]:=(start:Int, end:Int, step:Int, list:ListView<T>) {
        for i, j in start ... end by step
            self[j] := list[i]
    }
}
