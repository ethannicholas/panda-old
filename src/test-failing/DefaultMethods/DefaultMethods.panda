interface Interface1 {
    method hello() {
        Console.writeLine("Hello from interface1!")
    }
}

interface Interface2 {
    method hello() {
        Console.writeLine("Hello!")
    }

    method greet()
}

class Test (Interface1, Interface2) {
    @override
    method greet() {
        Console.writeLine("Also hello!")
    }

    @class
    method main() {
        var test := new Test()
        test.hello()
        test.greet()
    }
}