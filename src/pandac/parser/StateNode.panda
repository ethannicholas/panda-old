package org.pandalanguage.pandac.parser

class StateNode : Value {
	def state:Int
	def output:Object?
	def next:StateNode?

	constructor(state:Int, output:Object, next:StateNode?) {
		self.state := state
		self.output := output
		self.next := next
	}

	@override
	function format(fmt:String):String {
		if next != null
			return state + ", " + next
		return state->>String
	}
}
