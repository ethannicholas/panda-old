package org.pandalanguage.pandac.stubs

uses org.pandalanguage.pandac.types.Type

class TypeSymbol : Immutable (Symbol) {
	property type:Type

	init(type:Type) {
		self._type := type
	}

	@override
	function name():String {
		return type.name
	}

	@override
	function convert():String {
		return name
	}
}