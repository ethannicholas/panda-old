package org.pandalanguage.pandac.tree

uses org.pandalanguage.pandac.types.ClassType

class Construct : Value {
    def constructorCall:MethodCall

    constructor(constructorCall:MethodCall) {
        super.constructor(constructorCall.position, 
                constructorCall.methodStub.owner)
        self.constructorCall := constructorCall
    }
    
    @override
    function format(fmt:String):String {
        return constructorCall->>String
    }
}
