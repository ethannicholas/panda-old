package org.pandalanguage.pandac.tree

class While : AbstractLoop {
    def test:Value
    
    constructor(position:Position, label:String?, test:Value, 
            statement:Statement) {
        super.constructor(position, label, statement)
        self.test := test
    }
    
    @override
    function format(fmt:String):String {
        return "while \{test} \{statement}"
    }
}