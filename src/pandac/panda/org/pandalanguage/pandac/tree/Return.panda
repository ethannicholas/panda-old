package org.pandalanguage.pandac.tree

class Return : Statement {
    def value:Value?

    init(position:Position) {
        super.init(position)
    }
    
    init(position:Position, value:Value) {
        super.init(position)
        self.value := value
    }

    @override
    function ->>():String {
        var v := value
        if v != null
            return "return " + v
        else
            return "return"
    }
}