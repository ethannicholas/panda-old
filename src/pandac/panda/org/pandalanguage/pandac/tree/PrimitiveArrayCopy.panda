package org.pandalanguage.pandac.tree

uses org.pandalanguage.pandac.types.PrimitiveArrayType

class PrimitiveArrayCopy : Value {
    def array:Value
    
    constructor(position:Position, t:PrimitiveArrayType, array:Value) {
        super.constructor(position, t)
        self.array := array
    }
    
    @override
    function format(fmt:String):String {
        return "new " + type.name + "(" + array + ")"
    }
}