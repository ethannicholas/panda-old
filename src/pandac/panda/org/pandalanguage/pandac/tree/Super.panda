package org.pandalanguage.pandac.tree

uses org.pandalanguage.pandac.types.SuperType

class Super : Value {
    constructor() {
        super.constructor(new SuperType(Position.INTERNAL))
    }

    constructor(position:Position) {
        super.constructor(position, new SuperType(position))
    }

    @override
    function format(fmt:String):String {
    	return "super"
    }
}