package org.pandalanguage.pandac.tree

class BitwiseAnd : NumericBinaryOperation {
    constructor() {
        super.constructor()
    }
    
    constructor(position:Position) {
        super.constructor(position)
    }
    
    @override
    function signMatters():Bit {
        return false
    }

    @override
    function operator():String {
        return "&"
    }
}