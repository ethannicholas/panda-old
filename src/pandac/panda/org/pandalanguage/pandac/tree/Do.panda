package org.pandalanguage.pandac.tree

class Do : AbstractLoop {
    def test:Value

    init(position:Position, label:String?, test:Value, 
            statement:Statement) {
        super.init(position, label, statement)
        self.test := test
    }
    
    @override
    function ->>():String {
        return "do \{statement} while \{test}"
    }
}