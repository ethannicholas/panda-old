package org.pandalanguage.pandac.tree

class Continue : Statement {
    def label:String?
    
    init(position:Position) {
        super.init(position)
    }
    
    init(position:Position, label:String?) {
        super.init(position)
        self.label := label
    }

    @override
    function convert():String {
        var label := self.label
        if label = null {
            return "continue"
        }
        else {
            return "continue " + label
        }
    }
}