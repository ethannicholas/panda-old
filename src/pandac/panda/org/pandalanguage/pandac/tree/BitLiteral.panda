package org.pandalanguage.pandac.tree

uses org.pandalanguage.pandac.types.BitType

class BitLiteral : Value {
    def value:Bit
    
    init(position:Position, value:Bit) {
        super.init(position, BitType.BIT)
        self.value := value
    }
    
    @override
    function ->>():String {
        return value->>String
    }
}