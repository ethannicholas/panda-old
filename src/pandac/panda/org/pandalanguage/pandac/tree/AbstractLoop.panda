package org.pandalanguage.pandac.tree

@abstract
class AbstractLoop : Statement {
    def label:String?
    
    constructor(position:Position) {
        super.constructor(position)
    }
    
    constructor(position:Position, label:String?, children:ListView<Node>) {
        super.constructor(position, children)
        self.label := label
    }
    
    function block():Block {
        return self[0]->(Block)
    }
}