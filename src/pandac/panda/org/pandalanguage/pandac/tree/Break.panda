package org.pandalanguage.pandac.tree

class Break : Statement {
    def label:String?

    constructor(position:Position) {
        super.constructor(position)
    }
    
    constructor(position:Position, label:String?) {
        super.constructor(position)
        self.label := label
    }
    
    @override
    function ->>():String {
        var label := self.label
        if label = null
            return "break"
        else
            return "break " + label
    }
}