package org.pandalanguage.pandac.types

uses org.pandalanguage.pandac.compiler.CompilerException
uses org.pandalanguage.pandac.tree.Position

class UnresolvedTypeException : CompilerException {
    constructor() {
        super.constructor("unresolved type")
    }

    constructor(position:Position) {
        super.constructor("unresolved type", position)
    }

    constructor(name:String, position:Position) {
        super.constructor("unknown identifier '" + name + "'", position)
    }
}