package org.pandalanguage.pandac.ast

uses org.pandalanguage.pandac.tree.Position

class ASTIndex : ASTExpression {
    def target:ASTExpression
    def index:ASTExpression

    constructor(target:ASTExpression, index:ASTExpression) {
        super.constructor(target.position)
        self.target := target
        self.index  := index
    }    

    @override
    function ->>():String {
        return target + "[" + index + "]"
    }
}