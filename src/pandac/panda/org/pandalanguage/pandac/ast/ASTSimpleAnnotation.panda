package org.pandalanguage.pandac.ast

uses org.pandalanguage.pandac.tree.Position

class ASTSimpleAnnotation : ASTAnnotation {
    def name:String

    @pre(!name.startsWith("@"))
    constructor(position:Position, name:String) {
        super.constructor(position)
        self.name := name
    }

    @override
    function ->>():String {
        return name
    }

    @override
    method write(out:IndentedOutputStream) {
        out.writeLine(self->>String)
    }
}