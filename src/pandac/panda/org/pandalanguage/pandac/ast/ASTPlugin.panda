package org.pandalanguage.pandac.ast

uses org.pandalanguage.pandac.tree.Position

class ASTPlugin : ASTExpression {
    def value:String

    constructor(position:Position, value:String) {
        super.constructor(position)
        self.value := value
    }    

    @override
    function ->>():String {
        return "#/" + value + "/#"
    }
}