package org.pandalanguage.pandac.ast

uses org.pandalanguage.pandac.tree.Position

class ASTPackageDeclaration : ASTNode {
    def name:String

    init(position:Position, name:String) {
        super.init(position)
        self.name := name
    }    

    @override
    function ->>():String {
        return "package " + name
    }

    @override
    method write(out:IndentedOutputStream) {
        out.writeLine(self)
    }
}