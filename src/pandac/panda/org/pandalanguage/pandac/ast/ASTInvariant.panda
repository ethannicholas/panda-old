package org.pandalanguage.pandac.ast

uses org.pandalanguage.pandac.tree.Position

class ASTInvariant : ASTExpression {
    def value:ASTExpression

    init(position:Position, value:ASTExpression) {
        super.init(position)
        self.value := value
    }

    @override
    function ->>():String {
        return "@invariant(" + value + ")"
    }
}