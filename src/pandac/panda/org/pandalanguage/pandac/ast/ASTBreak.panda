package org.pandalanguage.pandac.ast

uses org.pandalanguage.pandac.tree.Position

class ASTBreak : ASTStatement {
    def label:String?

    init(position:Position, label:String?) {
        super.init(position)
        self.label := label
    }

    @override
    function ->>():String {
        if label != null
            return "break " + label
        else
            return "break"
    }
}