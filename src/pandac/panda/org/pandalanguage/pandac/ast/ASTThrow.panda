package org.pandalanguage.pandac.ast

uses org.pandalanguage.pandac.tree.Position

class ASTThrow : ASTStatement {
    def error:ASTExpression

    constructor(position:Position, error:ASTExpression) {
        super.constructor(position)
        self.error := error
    }    

    @override
    function ->>():String {
        return "throw " + error
    }
}