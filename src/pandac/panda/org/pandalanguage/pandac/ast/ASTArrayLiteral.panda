package org.pandalanguage.pandac.ast

uses org.pandalanguage.pandac.tree.Position

class ASTArrayLiteral : ASTExpression {
    def values:ImmutableArray<ASTExpression>

    constructor(position:Position, values:ListView<ASTExpression>) {
        super.constructor(position)
        self.values := new ImmutableArray<ASTExpression>(values)
    }

    @override
    function ->>():String {
        return "[" + values.join(", ") + "]"
    }
}