package org.pandalanguage.pandac.ast

uses org.pandalanguage.pandac.tree.Position

class ASTContinue : ASTStatement {
    def label:String?

    init(position:Position, label:String?) {
        super.init(position)
        self.label := label
    }    

    @override
    function ->>():String {
        if label != null
            return "continue " + label
        else
            return "continue"
    }
}