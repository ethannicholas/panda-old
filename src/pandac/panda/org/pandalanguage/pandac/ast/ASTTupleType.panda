package org.pandalanguage.pandac.ast

uses org.pandalanguage.pandac.tree.Position

class ASTTupleType : ASTType {
    def types:ImmutableArray<ASTType>

    constructor(position:Position, types:ListView<ASTType>, isNullable:Bit) {
        super.constructor(position, isNullable)
        self.types := new ImmutableArray<ASTType>(types)
    }    
}