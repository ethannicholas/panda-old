package org.pandalanguage.pandac.ast

uses org.pandalanguage.pandac.tree.Position

class ASTRealLiteral : ASTExpression {
    def value:Real64

    init(position:Position, value:Real64) {
        super.init(position)
        self.value := value
    }    

    @override
    function ->>():String {
        return value->>String
    }
}