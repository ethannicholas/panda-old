package org.pandalanguage.pandac.ast

uses org.pandalanguage.pandac.tree.Position

class ASTClassLiteral : ASTExpression {
    def type:ASTType

    constructor(type:ASTType) {
        super.constructor(type.position)
        self.type := type
    }    

    @override
    function ->>():String {
        return type->>String
    }
}