package org.pandalanguage.pandac.ast

uses org.pandalanguage.pandac.tree.Position

class ASTFile : ASTNode {
    def name:String

    def entries:ImmutableArray<ASTNode>

    constructor(name:String, entries:ListView<ASTNode>) {
        super.constructor(new Position(name, 1, 1))
        self.name    := name
        self.entries := new ImmutableArray<ASTNode>(entries)
    }

    @override
    method write(out:IndentedOutputStream) {
        for e in entries
            e.write(out)
    }    
}