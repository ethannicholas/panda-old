package org.pandalanguage.pandac.ast

uses org.pandalanguage.pandac.tree.Position

class ASTBitLiteral : ASTExpression {
    def value:Bit

    init(position:Position, value:Bit) {
        super.init(position)
        self.value := value
    }

    @override
    function convert():String {
        return value->>String
    }
}