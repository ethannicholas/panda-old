package org.pandalanguage.pandac.ast

uses org.pandalanguage.pandac.tree.Position

class ASTBitLiteral : ASTExpression {
    def value:Bit

    constructor(position:Position, value:Bit) {
        super.constructor(position)
        self.value := value
    }

    @override
    function ->>():String {
        return value->>(String)
    }
}