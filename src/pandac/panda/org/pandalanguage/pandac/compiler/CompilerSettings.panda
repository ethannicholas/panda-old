package org.pandalanguage.pandac.compiler

class CompilerSettings : Immutable {
    def sources:ImmutableArray<File>

    def preserveTempArtifacts := true

    def librarySearchPaths:ImmutablePrimitiveArray<File> := []

    def libraries:ImmutablePrimitiveArray<String> := []

    def modules:ImmutablePrimitiveArray<Module> := []

    def objectFiles:ImmutablePrimitiveArray<File> := []

    constructor(sources:ListView<File>) {
        self.sources := new ImmutableArray<File>(sources)
    }
}