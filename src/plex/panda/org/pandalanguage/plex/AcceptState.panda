package org.pandalanguage.plex

class AcceptState : State {
    var token:Int

    constructor(token:Int) {
        super.constructor([])
        self.token := token
    }

    @override
    function accept(c:Char):Bit {
        return false
    }

    @override
    function ->>():String {
        return "new AcceptState(" + token + ")"
    }
}