class Super {
    method hello() {
        Console.writeLine("\{class.name} calling Super.hello()!")
    }
}

class Sub : Super {
    @override
    method hello() {
        Console.writeLine("\{class.name} calling Sub.hello()!")
    }
}

def sub := new Sub()
sub.hello()

def hello := Super::hello
hello(sub)
hello(new Super())
