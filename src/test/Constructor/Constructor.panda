class ConstructorSuperTest {
    var s:String
    
    constructor() {
        s := "initialized"
    }
}

class ConstructorTest1 : ConstructorSuperTest {
    -- expecting default constructor which calls super.constructor()
}

class ConstructorTest2 : ConstructorSuperTest {
    constructor() {
        -- expecting default super.constructor()
    }
}

var c1 := ConstructorTest1()
Console.writeLine(c1.class.name)
Console.writeLine(c1.s)

var c2 := ConstructorTest2()
Console.writeLine(c2.class.name)
Console.writeLine(c2.s)