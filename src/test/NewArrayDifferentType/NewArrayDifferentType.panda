var a:ImmutableArray<String> := ["Hello", "Goodbye"]
var b := new Array<Object>(a, 0, a.length)

assert a.length = b.length
for i, e in a
    assert b[i] = e

var c:ImmutableArray<Int32> := [1, 5, 7, 12, -52]
var d := new Array<Int32>(c, 0, c.length)

assert c.length = c.length
for i, e in c
    assert d[i] = e

Console.writeLine("success")
