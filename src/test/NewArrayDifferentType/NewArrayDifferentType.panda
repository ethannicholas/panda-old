var a:ImmutablePrimitiveArray<String> := ["Hello", "Goodbye"]
var b := new PrimitiveArray<Object>(a, 0, a.length)

assert a.length = b.length
for (i, e) in a.enumeration()
    assert b[i] = e

var c:ImmutablePrimitiveArray<Int32> := [1, 5, 7, 12, -52]
var d := new PrimitiveArray<Int32>(c, 0, c.length)

assert c.length = c.length
for (i, e) in c.enumeration()
    assert d[i] = e

