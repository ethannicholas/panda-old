def a:Char := "A"
assert a in ["A", "B", "C"]
assert !(a in ["B", "C", "D"])
assert a in "A" ... "C"
assert !(a in "D" ... "G")

def b:String := "A"
assert b in ["A", "B", "C"]
assert !(b in ["X"])

def c:Real32 := 2.5
assert c in [1, 2.5, 5.75]
assert !(c in [1, 2.55, 5.75])
assert c in 1 ... 5 by 0.5
assert !(c in 1 ... 5 by 0.6)

def d:Real64 := 2.5
assert d in [1, 2.5, 5.75]
assert !(d in [1, 2.55, 5.75])
assert d in 1 ... 5 by 0.5
assert !(d in 1 ... 5 by 0.6)

assert [1, 2] in [[1, 3], [1, 2]]
assert !([1, 2] in [[1, 3], [2, 2]])
assert !([1, 2] in [[1, 3], [1, "A"]])
assert 1 .. 5 in [1 .. 5, 1, 7]
assert !(1 .. 5 in [1 .. 6, 1, 7])
assert 1 .. 5 in [1 .. 5, 1, "A"]

assert ("Hello", 1) in [("Hello", 1), ("Goodbye", 2)]
assert !(("Hello", 1) in [("Hello", 2), ("Goodbye", 1)])

Console.writeLine("success")
