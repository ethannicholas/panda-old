var i8:Int8 := 52
var i8n:Int8? := i8
assert i8n = 52
var i16:Int16 := i8n
assert i16 = 52
var i16n:Int16? := (i16 - 10)->(Int16)
assert i16n = 42
i16n := i8
assert i16n = 52
i16n := i8n
assert i16n = 52
i8n := (i16n->(Int8?) - 20)->(Int8)
assert i8n = 32

