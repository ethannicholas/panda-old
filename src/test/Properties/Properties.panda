class Super {
    property name := "<no name>"
}

class Sub : Super {
    @override
    function name():String {
        return _name + "OVERRIDDEN"
    }

    @override
    @self
    method name:=(name:String) {
        Console.writeLine("setting name to: \{name}")
        super.name := name
    }
}

method main() {
    var s1 := Super()
    s1.name := "success"

    var s2 := Sub()
    s2.name := "test"
    Console.writeLine(s2.name)
    Console.writeLine(s1.name)
}