method writeAll(strings:String...) {
    for i in 0 .. strings.length
        Console.writeLine(strings[i])
}

writeAll("Hello", "Goodbye")
def array := new Array<String>()
array.add("Hello2")
array.add("Goodbye2")
writeAll(array...)
writeAll(["Hello3", "Goodbye3"]...)
writeAll()

class VariadicConstructor {
    constructor(int:Int, ints:Int...) {
        Console.writeLine("received: \{ints[0]}, \{ints[1]}")
        assert ints.length = 2
    }
}

def result := new VariadicConstructor(13, 42, 85)