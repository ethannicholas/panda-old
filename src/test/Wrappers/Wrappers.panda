function type(o:Object):String {
    return o.class.name
}

assert type("C") = "panda.core.String"
var c:Char? := "C"
assert c = "C"
assert "C" = c
assert c != "D"
assert "D" != c
assert c = "C"->(String)
assert "C"->(String) = c
assert c != "D"->(String)
assert "D"->(String) != c
assert c = "C"->(Char) 
assert "C"->(Char)  = c
assert c != "D"->(Char) 
assert "D"->(Char)  != c
c := null
assert c != "C"
assert "C" != c
assert c != "C"->(String)
assert "C"->(String) != c
assert c != "C"->(Char) 
assert "C"->(Char)  != c
assert c = null
assert null = c

var i:Int? := 5
assert i = 5
assert 5 = i
assert i != 10
assert 10 != i
assert i < 10
assert 2 < i
i := null
assert i != 5
assert 5 != i
assert i = null
assert null = i

var b:Bit? := true
assert b = true
assert true = b
assert b != false
assert false != b
b := null
assert b != true
assert true != b
assert b = null
assert null = b

var r:Real? := 3.25
assert r = 3.25
assert 3.25 = r
assert r != 3.3
assert 3.3 != r
r := null
assert r != 3.25
assert 3.25 != r
assert r = null
assert null = r

Console.writeLine("success")
