var s := "Hello, World!"->>MutableString
s[..5] := "!Goodbye!"[1..8]
assert s->>String = "Goodbye, World!"
s[..] := s[.. by -1]
assert s->>String = "!dlroW ,eybdooG"
s[.. by -1] := "!dlroW ,eybdooG"
assert s->>String = "Goodbye, World!"

var a := (0 .. 100 by 10)->>PrimitiveArray<Int>
assert a[5] = 50
a[..] := 0 .. 5
assert a.length = 5
for i, v in a
    assert i = v

a := [0, 0, 0, 0, 0, 0, 0]
a[2.. by 2] := [1, 3, 5]
Console.writeLine(a)