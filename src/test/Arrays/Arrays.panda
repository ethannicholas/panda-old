var s := new Array<String>("Goodbye", "World")
assert s.length = 2
s[0] := "Hello"
assert s[0] + " " + s[1] = "Hello World"

var a := new Array<Int>(10)
assert a[0] = 0
a[9] := 42
assert a[9] = 42

var grow := new Array<Int>()
for index in 0 .. 10001
    grow.append(index)
    
for index in 0 .. 10001
    assert grow[index] = index

var nested := new Array<Array<Array<Int>>>()
for i in 0 .. 5 {
    nested.append(new Array<Array<Int>>())
    for j in 0 .. 5 {
        nested[i].append(new Array<Int>())
        for k in 0 .. 5
            nested[i][j].append(i * 100 + j * 10 + k)
    }
}
assert nested[3][1][2] = 312
assert nested[1][0][4] = 104

Console.writeLine("Success")