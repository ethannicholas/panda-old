class Test {
    @pre(i > 10)
    method test(i:Int) {
        Console.writeLine(i)
    }
}

class Test2 : Test {
    @override
    @preOr(i > 0)
    method test(i:Int) {
        Console.writeLine(i)
    }
    
    @class
    @pre(i < 0)
    method test2(i:Int) {
        Console.writeLine(i)
    }
}

var t := new Test2()
try {
    t.test(-1)
}
catch e:AssertionError {
    Console.writeLine("success")
}
