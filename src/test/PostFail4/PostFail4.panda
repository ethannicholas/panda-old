class Test1 {
    var i:Int
    
    method set(i:Int) {
        self.i := i
    }
    @post(i > 10)
}

class Test2 : Test1 {
    @override
    method set(i:Int) {
        super.set(i)
    }
    @postAnd(i > 0)
}

var t := new Test2()
t.set(12)
Console.writeLine(t.i)
try {
    t.set(8)
    Console.writeLine(t.i)
}
catch e:AssertionError {
    Console.writeLine("success")
}