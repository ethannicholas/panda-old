-- currently only convert works with super, the rest will come eventually

class A {
    function +(s:String):String {
        return self->>String + s
    }

    @override
    function convert():String {
        return "ConvertA"
    }
}

class B : A {
    @override
    function convert():String {
        return super->>String + "ConvertB"
    }

    @class
    method main() {
        Console.writeLine(B())
    }
}