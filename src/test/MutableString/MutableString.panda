def m := new MutableString("Hello")
assert m->>(String) = "Hello"
m[..] := "Goodbye"
assert m->>(String) = "Goodbye"
m[0 ... 3] := "Well, "
assert m->>(String) = "Well, bye"
m.append("!")
assert m->>(String) = "Well, bye!"
m[6 .. 9] := "hello"
assert m->>(String) = "Well, hello!"
m[11 .. 11] := " there"
assert m->>(String) = "Well, hello there!"

assert m.startsWith("Well")
assert !m.startsWith("Hello")
assert m.startsWith("")
assert !m.startsWith("Well, hello there!!")

assert m.endsWith("!")
assert m.endsWith("")
assert !m.endsWith("!!")
assert !m.startsWith(" Well, hello there!")

assert m.contains("Well")
assert m.contains("")
assert !m.contains("Hello")

assert m[0 .. 4] = "Well"
assert m[0 ... 3] = "Well"
assert m[3 .. -1 by -1] = "lleW"
assert m[3 ... 0 by -1] = "lleW"
assert m[3 .. 0] = ""
assert m[0 .. 4 by 2] = "Wl"

Console.writeLine("success")