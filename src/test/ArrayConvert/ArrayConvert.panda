class Test : Immutable {
    def value:Int

    constructor(value:Int) {
        self.value := value
    }

    @override
    function format(fmt:String):String {
        return value->>String
    }
}

var a:Array<Test> := [new Test(10), new Test(12)]
var b:Array<Test?> := [new Test(13), new Test(-52)]

Console.writeLine(a->>ImmutableArray<Test>)
Console.writeLine(b->>ImmutableArray<Test?>)

var c:ImmutableArray<Test> := [new Test(76), new Test(127)]
var d:ImmutableArray<Test?> := [new Test(43), new Test(131)]

Console.writeLine(c->>Array<Test>)
Console.writeLine(d->>Array<Test?>)