class Test : Immutable {
    def value:Int

    init(value:Int) {
        self.value := value
    }

    @override
    function convert():String {
        return value.convert()
    }
}

var a:Array<Test> := [Test(10), Test(12)]
var b:Array<Test?> := [Test(13), Test(-52)]

Console.writeLine(a.convert()->ImmutableArray<Test>)
Console.writeLine(b.convert()->ImmutableArray<Test?>)

var c:ImmutableArray<Test> := [Test(76), Test(127)]
var d:ImmutableArray<Test?> := [Test(43), Test(131)]

Console.writeLine(c.convert()->Array<Test>)
Console.writeLine(d.convert()->Array<Test?>)