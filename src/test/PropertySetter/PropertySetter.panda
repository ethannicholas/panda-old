class PropertySetter {
    var children:Array<String?> := new Array<String?>()
    
    function length():Int {
        return children.length
    }

    @self
    method length:=(i:Int) {
        children.length := i
    }

    @self
    method noGetter:=(s:String) {
        Console.writeLine("received: \{s}")
    }
}

var p := new PropertySetter()
p.children.append("Test")
Console.writeLine(p.length)
p.length -= 1
Console.writeLine(p.length)
p.noGetter := "Hello!"
Console.writeLine("done")